{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["import"],
  "rules": {
    "no-unused-vars": "warn"
  },
  "ignorePatterns": ["node_modules/**/*", "webpack.*", "dist/*"],
  "overrides": [
    {
      "files": "**/*.test.js",
      "env": {
        "jest": true
      }
    }
  ]
}
